{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@Types": ["src/Types.ts"],
      "@Components": ["src/components/"],
      "@Components/*": ["src/components/*"],
      "@Models": ["src/models/"],
      "@Models/*": ["src/models/*"],
      "@Hooks": ["src/hooks/"],
      "@Hooks/*": ["src/hooks/*"],
      "@Utils": ["src/utils/"],
      "@Utils/*": ["src/utils/*"],
      "@PComponents": ["src/pages/Pinit/components/"],
      "@PComponents/*": ["src/pages/Pinit/components/*"],
      "@PModels": ["src/pages/Pinit/models/"],
      "@PModels/*": ["src/pages/Pinit/models/*"],
      "@PUtils": ["src/pages/Pinit/utils/"],
      "@PUtils/*": ["src/pages/Pinit/utils/*"]
    }
  },
  "include": ["./src/**/*"]
}
